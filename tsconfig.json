{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "outDir": "/dev/null"
  },
  "filesGlob": [
    "bower_components/**/*.d.ts",
    "typings/**/*.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "bower_components"
  ]
}
